{
  "name": "@miniprogram-tools/storage",
  "version": "1.0.6",
  "type": "module",
  "miniprogram": "dist",
  "repository": {
    "type": "git",
    "url": "https://github.com/miniprogram-tools/storage"
  },
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "development": "./src/index.ts",
        "production": "./dist/index.js",
        "default": "./dist/index.js"
      },
      "require": {
        "development": "./dist/index.cjs",
        "production": "./dist/index.cjs",
        "default": "./dist/index.cjs"
      },
      "types": "./dist/index.d.ts"
    }
  },
  "keywords": [
    "wxchat",
    "storage",
    "cache",
    "miniprogram-tools"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "buildOptions": {
    "name": "MiniprogramStorage",
    "formats": [
      "es",
      "cjs"
    ],
    "entry": "src/index.ts"
  },
  "publishConfig": {
    "access": "public"
  }
}